{
    "python.pythonPath": "./pyenv/bin/python",
    "workbench.editor.enablePreview": false,
    "explorer.autoReveal": false,
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "python.linting.ignorePatterns": [
        ".vscode/*.py",
        "transcrypt/python/**/*",
    ],
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--max-line-length",
        "150",
        "--ignore",
        "E401, E402, E501, E701"
    ],
    "editor.formatOnSave": false,
    "workbench.tree.indent": 14,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "files.exclude": {
        "**/__pycache__/**": true,
        "**/pyenv/**": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        ".pytest_cache": true,
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "build": true,
        "dist": true,
        "docs/sphinx/build": true,
        "notebooks": true,
        "transcrypt/node_modules": true,
        "transcrypt/python/__target__": true,
        "audio_controller/audio_controller/static/js/bootstrap-4.3.1-dist": true,
        "audio_controller/audio_controller/static/js/fontawesome-free-5.10.1-web": true,
    },
    "files.watcherExclude": {
        "**/__pycache__/**": true,
        "**/pyenv/**": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        ".pytest_cache": true,
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "build": true,
        "dist": true,
        "docs/sphinx/build": true,
        "notebooks": true,
        "transcrypt/node_modules": true,
        "transcrypt/python/__target__": true,
        "audio_controller/audio_controller/static/js/bootstrap-4.3.1-dist": true,
        "audio_controller/audio_controller/static/js/fontawesome-free-5.10.1-web": true,
    },
    "restructuredtext.confPath": "",
}